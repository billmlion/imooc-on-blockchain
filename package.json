{
  "name": "imooc-on-blockchain",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "2015": "0.0.1",
    "antd": "^3.25.1",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.0",
    "ganache-cli": "^6.7.0",
    "ipfs-api": "^26.1.2",
    "mocha": "^6.2.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "rimraf": "^3.0.0",
    "solc": "^0.4.24",
    "truffle-hdwallet-provider": "0.0.3",
    "web3": "^1.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "node scripts/deploy.js",
    "precompile": "rimraf src/compiled/*",
    "compile": "node scripts/compile.js",
    "precompile:w": "npm run compile",
    "compile:w": "onchange contracts/*.sol -- npm run compile",
    "test:w": "mocha --watch",
    "rebuild": "npm run compile && mocha",
    "prerebuild:w": "npm run rebuild",
    "rebuild:w": "onchange contracts/*.sol test/*.js -- npm run rebuild"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
